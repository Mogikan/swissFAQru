{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "wohnung/**.yml",
          "wohnung/**.md"
        ]
      },
      {
        "files": [
          "transport/**.md",
          "transport/**.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "schule/**.md",
          "schule/**.yml"
        ]
      },
      {
        "files": [
          "driver_license/**.md",
          "driver_license/**.yml"
        ]
      }
      
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}